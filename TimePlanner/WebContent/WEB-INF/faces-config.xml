<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>userCriteria</managed-bean-name>
  <managed-bean-class>de.tuberlin.hdis14.ui.UserCriteria</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/backgroundpages/start.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>greeting</from-outcome>
   <to-view-id>/backgroundpages/greeting.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <resource-bundle>
   <base-name>resources</base-name>
   <var>msgs</var>
  </resource-bundle>
 </application>
</faces-config>
